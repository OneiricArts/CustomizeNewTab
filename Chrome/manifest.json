{
	"manifest_version": 2,
	"name": "Sports New Tab Page (Beta)",
	"version": "0.9.8.6",
	"chrome_url_overrides": {
		"newtab": "src/new_tab.html"
	},
	"permissions": [
		"storage",
		"http://www.nfl.com/",
		"http://*.nba.com/*",
		"http://*.nhle.com/*",
		"http://*.mlb.com/*",
		"https://*.unsplash.com/*",
		"topSites"
	],
	"icons": { "16": "icon16.png",
          "128": "icon128.png" },
    "content_security_policy": "script-src 'self' https://www.google-analytics.com/analytics.js 'unsafe-eval'; object-src 'self'"
}
