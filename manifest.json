{
	"manifest_version": 2,
	"name": "Sports New Tab Page (Beta)",
	"version": "0.1",
	"chrome_url_overrides": {
		"newtab": "new_tab.html"
	},
	"permissions": [
		"storage",
		"http://www.nfl.com/",
		"<all_urls>"
	],
	"icons": { "16": "icon16.png",
          "128": "icon128.png" },
  	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}