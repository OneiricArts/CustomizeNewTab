<!-- Links -->
			
{{#if links.customSites}}
<div id="custom-sites" class="list-group">
	<a href="#" class="list-group-item disabled">
		Custom Links</a>
	{{#each links.customSites}}
		<a href="{{url}}" id="{{@index}}_customLink"
		class="list-group-item">{{title}}
				<button id="remove_customLink"
					class="btn btn-xs btn-default pull-right">
					<span class="glyphicon glyphicon-trash"></span> 
				</button>
			</a>
	{{/each}}
</div>
{{/if}}

<div id="top-sites" class="list-group">
	<a href="#" class="list-group-item disabled">
		Chrome Top Sites</a>
	{{#each links.topSites}}
		<a href="{{url}}"
		class="list-group-item">{{title}}</a>
	{{/each}}
</div>


<span data-toggle="modal" data-target="#addCustomLinkModal">
<button type="button" class="btn btn-default pull-right" 
data-toggle="tooltip" data-placement="left" title="Add Custom Link"
style="height: 40px; width:40px; line-height: 22px; vertical-align:middle; border-radius:50%; padding:0;">
{{!-- style="height: 34px; line-height: 16px; vertical-align:middle; border-radius:15%;"> --}}
<span style="font-size:25px;">&#43;</span></button> 
</span>

